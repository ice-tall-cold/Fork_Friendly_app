<h1 class="divText" class="jumbotron">Add Files To Grocery's Database</h1>
<div id="top">
	<%= csrf_meta_tags %>
	<div id="logout">
        <%= link_to "Log Out", logout_path, method: "delete" %>
    </div>
    <div id="concernslink">
        <%= link_to "Concerns", health_concerns_concerns_path %>
    </div>
</div>
<center>
    <%= form_tag import_product_lines_path, multipart: true do %>
    
        <div id="ChooseInputFile">
            <%= file_field_tag :file, :id =>'Choose File' %>
        
        </div>
        
        <div id="startShopping">
            <%= submit_tag("Import", :class => 'btn btn-primary center col-xs-2 col-xs-offset-5 margin-tb-t', :id => 'newsubmitButton') %>
        </div>

<% end %>  
<br>
</center>